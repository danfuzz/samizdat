##
## Symbol literal demo
##

#= language core.Lang0


##
## Private Definitions
##

fn msg(string) {
    note("Got: ", $Format::source(string))
};


##
## Main Tests
##

note("Symbols");
msg(@.foo);
msg(@."foo");
msg(@."++ blort ++");

ifNot { eq(@.foo, @."foo") }
    { die("Expected same!") };

def anon = @.foo.makeAnonymous();
ifIs { eq(@.foo, anon) }
    { die("Expected interned and anonymous to be different!") };

ifIs { eq(anon, anon.makeAnonymous()) }
    { die("Expected anonymous and anonymous to be different!") };
