##
## Private Definitions
##

## Prints out the given value.
fn msg(value) {
    note("\%q(value)");
};


##
## Main Tests
##

note("Layer 0:\n");

msg([1, 2].get_size());
msg(["zero", "one", "two"].get(0));
msg(["zero", "one", "two"].nth(1));
msg((100).perEq(100));

note("\nLayer 2:\n");

def theSecret = @secret.toUnlisted();
class Igram
        access: theSecret {
    fn get_foo() {
        return "a foo"
    };
    fn set_foo(foo) {
        note("Set to: \%q(foo)");
    };
};

def obj = theSecret(Igram);
note(obj.foo);
obj.foo := "whatevs";
