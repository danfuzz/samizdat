##
## Private Definitions
##

## Prints out the given value.
fn msg(value) {
    note("\%q(value)");
};


##
## Main Tests
##

note("Layer 0:\n");

msg([1, 2].get_size());
msg(["zero", "one", "two"].get(0));
msg(["zero", "one", "two"].nth(1));
msg((100).perEq(100));

note("\nLayer 2:\n");

def get_foo = @.get_foo;
def set_foo = @.set_foo;
def theSecret = "fizmo";
def Igram = makeObjectClass("Igram", theSecret);
fn Igram.get_foo() {
    return "a foo"
};
fn Igram.set_foo(foo) {
    note("Set to: \%q(foo)");
};
def obj = makeObject(Igram, theSecret);
note(obj.foo);
obj.foo := "whatevs";
