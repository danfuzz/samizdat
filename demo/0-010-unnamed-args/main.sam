## Demo of unnamed arguments
##


##
## Helper Definitions
##

fn anon(.) {
    note("anon()")
};

fn anonArg(., arg) {
    note(cat("anonArg() got: ", $Format::source(arg)))
};

fn argAnon(arg, .) {
    note(cat("argAnon() got: ", $Format::source(arg)))
};

fn argAnonArg(arg1, ., arg2) {
    note("argAnonArg() got");
    note(cat("  ", $Format::source(arg1)));
    note(cat("  ", $Format::source(arg2)));
};


##
## Main Tests
##

anon("x");
anonArg("x", "blort");
argAnon("blort", "x");
argAnonArg("blort", "x", "igram");
