##
## `debugString` demo
##

#= language core.Lang0


##
## Private Definitions
##

## Print the debug-string of the given value.
fn msg(value) {
    note(value.debugString())
};


##
## Main Tests
##

msg(null);
msg(false);
msg(true);
msg("a String");
msg(10);
msg([1, 2, 3]);
msg({a: 1, b: 2});
msg(@blort{fizmo: "zorch"});

var foo = 10;
msg(var foo);  ## a box

msg(Value);                   ## a core class
msg(@@blort);                 ## a record class
msg(@blort);                  ## an interned symbol
msg(@blort.makeAnonymous());  ## an anonymous symbol
msg(eq);                      ## a builtin function
msg(msg);                     ## a named in-language function
msg({ -> });                  ## an anonymous in-language function

{ /out ->
    msg(out);  ## a nonlocal exit function
}();
