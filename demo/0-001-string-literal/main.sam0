#
# String literal demo
#

def Io0 = moduleUse({name: ["core", "Io0"]});


#
# Helper Definitions
#

fn msg(string) {
    Io0::note(cat("Got: ", Format::source(string)))
};


#
# Main Tests
#

msg("");
msg("a");
msg("fizmo");
msg("\0");
msg("\n");
msg("\r");
msg("\t");
msg("\"");
msg("\\");

<> 0
