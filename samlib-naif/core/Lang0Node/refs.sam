def NAMES = [
    "cat",
    "catCollect",
    "get",
    "ifIs",
    "ifNot",
    "ifValue",
    "ifValueOr",
    "ifValues",
    "loop",
    "makeList",
    "makeValue",
    "makeValueMap",
    "nonlocalExit",
    "optValue",
    "totEq",
    "totNe"
];

def REFS = cat(
    (Generator::filterAll(NAMES) { name <> {(name): @varRef{name}} })*);

<> {REFS}
