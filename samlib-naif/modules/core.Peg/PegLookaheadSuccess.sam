#= language core.Lang0


##
## Class Definition
##
## Payload: {rule: rule}

## Secret used to control access to this class.
def SECRET = @secret.toUnlisted();

class PegLookaheadSuccess
        access: SECRET {
    ## Documented in spec.
    fn parse(box, input, items*) {
        def data = SECRET(this);
        return? ifIs { data::rule.parse(box, input, items*) }
            { input }
    };
};


##
## Exported Definitions
##

## Documented in spec.
export fn makeLookaheadSuccess(rule) {
    return makeObject(PegLookaheadSuccess, SECRET, @{rule})
};
