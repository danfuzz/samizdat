##
## `PegResult` class
##

#= language core.Lang0


##
## Class Definition
##
## Payload: {value: .}

## Secret used to control access to this class.
def SECRET = @secret.makeAnonymous();

## Class being defined here.
def PegResult = makeObjectClass(@PegResult, SECRET);

## Documented in spec.
fn PegResult.parse(box, input, .*) {
    def data = this.objectDataOf(SECRET);
    box* := data::value;
    return input
};


##
## Exported Definitions
##

## Documented in spec.
export fn makeResult(value) {
    return makeObject(PegResult, SECRET, @{value})
};

## Documented in spec.
export def empty = makeResult(null);
