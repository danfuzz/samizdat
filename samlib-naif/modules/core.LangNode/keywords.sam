#= language core.Lang0

import core.Generator :: collectAsMap, makeFilterGenerator;
import ./constructors1 :: makeLiteral;


##
## Private Definitions
##

## List of the string names of all of the keywords.
def KEYWORD_NAMES = [
    ## Layer 0.
    "break", "continue", "def", "export", "false", "fn", "import", "null",
    "return", "true", "var", "yield",

    ## Layer 2.
    "by", "default", "do", "else", "for", "if", "in", "module", "switch",
    "while"
];


##
## Exported Definitions
##

## Documented in spec.
export def KEYWORDS = collectAsMap(
    makeFilterGenerator(KEYWORD_NAMES)
    { name -> {(name): @(@@(name))} });
