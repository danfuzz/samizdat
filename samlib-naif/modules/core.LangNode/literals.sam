#= language core.Lang0

import core.Generator :: filterAll;
import ./constructors1 :: makeLiteral;


##
## Private Definitions
##

## Literal symbols.
def SYMBOLS = [
    @add,
    @and,
    @cat,
    @collect,
    @div,
    @divEu,
    @exclusive,
    @exports,
    @get,
    @get_size,
    @inclusive,
    @info,
    @mod,
    @modEu,
    @mul,
    @neg,
    @not,
    @or,
    @perEq,
    @reverse,
    @shl,
    @sliceExclusive,
    @sliceInclusive,
    @shr,
    @sub,
    @value,
    @xor
];

## Other literals.
def OTHERS = {
    EMPTY_LIST: [],
    EMPTY_MAP:  {},
    false:      false,
    null:       null,
    true:       true
};


##
## Exported Definitions
##

## Documented in spec.
export def SYMS = OTHERS.cat(
    (filterAll(SYMBOLS) { name ->
        {(name): makeLiteral(name)}
    })*
);
