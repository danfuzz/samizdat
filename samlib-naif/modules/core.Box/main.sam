##
## Box functions
##

#= language core.Lang0

def $Box = moduleLoad(["proto", "Box"]);

def $NullBox = intraLoad("NullBox");


##
## Exported Definitions
##

## Re-exported from proto.
export def canStore = $Box::canStore;
export def fetch    = $Box::fetch;
export def store    = $Box::store;

## Re-exported from `NullBox`.
export def nullBox = $NullBox::nullBox;

## Re-exported from primitive library.
export def makeCell    = Box_makeCell;
export def makePromise = Box_makePromise;
export def makeResult  = Box_makeResult;
