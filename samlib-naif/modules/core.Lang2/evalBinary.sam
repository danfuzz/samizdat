##
## Samizdat Layer 2 Binary Library Support
##

def Io1   = moduleLoad(["core", "Io1"]);
def Lang1 = moduleLoad(["core", "Lang1"]);


##
## Exported Definitions
##

## Documented in spec.
fn evalBinary(env, filePath) {
    def flatPath = Io1::flatFromPath(filePath);
    <> Lang1::evalBinary(env, flatPath)
};

<> {
    evalBinary
}
