#
# String functions
#

# Documented in Samizdat Layer 0 spec.
fn stringFromTokenList(tokens) {
    <> listReduce("", tokens)
        { result, token :: <> stringAdd(result, tokenType(token)) };
};

<> [
    stringFromTokenList: stringFromTokenList
]
