def Io1 = moduleUse({name: ["core", "Io1"]});

fn compile(sourceFile, targetFile) {
    def sourcePath = Io1::pathFromFlat(sourceFile);
    def targetPath = Io1::pathFromFlat(targetFile);

    Io1::writeFileUtf8(targetPath, "TODO\n")
};

<> {compile: compile}
