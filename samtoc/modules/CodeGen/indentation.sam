##
## Indentation helper.
##

##
## Private Definitions
##

## List of indentation prefixes (index is level of indentation).
def INDENTATION = [ n in (0..200) -> "\[for n -> "  "]" ];


##
## Exported Definitions
##

export fn indentation(level) {
    return INDENTATION[level]
};
