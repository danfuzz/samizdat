##
## Indentation helper.
##

## List of indentation prefixes (index is level of indentation).
def INDENTATION = [ n in (0..100) <> "\[for n <> "  "]" ];

fn indentation(level) {
    <> INDENTATION[level]
};

<> {indentation}
